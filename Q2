#include <iostream>
using namespace std;

void DFS(int adj[][100], int n, int u, int visited[]){
    visited[u] = 1;
    cout << u << " ";

    for(int v = 0; v < n; v++)
        if(adj[u][v] == 1 && visited[v] == 0)
            DFS(adj, n, v, visited);
}

int main(){
    int n, adj[100][100], start;

    cout << "Enter number of vertices: ";
    cin >> n;

    cout << "Enter adjacency matrix:\n";
    for(int i = 0; i < n; i++)
        for(int j = 0; j < n; j++)
            cin >> adj[i][j];

    cout << "Enter starting vertex: ";
    cin >> start;

    int visited[100] = {0};
    cout << "DFS: ";
    DFS(adj, n, start, visited);
}
