#include <iostream>
using namespace std;

void BFS(int adj[][100], int n, int start){
    int visited[100] = {0};
    int q[100];
    int front = 0, rear = 0;

    visited[start] = 1;
    q[rear++] = start;

    while(front != rear){
        int u = q[front++];
        cout << u << " ";

        for(int v = 0; v < n; v++){
            if(adj[u][v] == 1 && visited[v] == 0){
                visited[v] = 1;
                q[rear++] = v;
            }
        }
    }
}

int main(){
    int n, adj[100][100], start;

    cout << "Enter number of vertices: ";
    cin >> n;

    cout << "Enter adjacency matrix:\n";
    for(int i = 0; i < n; i++)
        for(int j = 0; j < n; j++)
            cin >> adj[i][j];

    cout << "Enter starting vertex: ";
    cin >> start;

    cout << "BFS: ";
    BFS(adj, n, start);
}
